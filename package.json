{
  "name": "jj-stack-prs",
  "version": "0.1.0",
  "description": "Create GitHub Pull Requests from your Jujutsu stack",
  "main": "src/main.ts",
  "scripts": {
    "test": "deno test",
    "build": "deno compile --allow-run --allow-read --allow-write --allow-env --output=jj-stack-prs src/main.ts",
    "dev": "deno run --allow-run --allow-read --allow-write --allow-env src/main.ts",
    "lint": "deno lint",
    "fmt": "deno fmt"
  },
  "keywords": [
    "jujutsu",
    "jj",
    "github",
    "pull-request",
    "stack",
    "cli",
    "vcs",
    "git"
  ],
  "author": "Tommy Morgan",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tommymorgan/jj-tools.git"
  },
  "bugs": {
    "url": "https://github.com/tommymorgan/jj-tools/issues"
  },
  "homepage": "https://github.com/tommymorgan/jj-tools#readme"
}